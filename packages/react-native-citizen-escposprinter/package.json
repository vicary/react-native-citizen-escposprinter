{
  "name": "react-native-citizen-escposprinter",
  "version": "0.2.4",
  "description": "A Turbo Module wrapper for the Citizen ESC POS Printer SDK.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "tsc --noEmit && jest --collectCoverage",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "kotlin",
    "swift",
    "citizen",
    "ESC",
    "POS",
    "escpos",
    "opos",
    "wifi",
    "bluetooth",
    "printer",
    "receipt",
    "thermal"
  ],
  "repository": "https://github.com/vicary/react-native-citizen-escposprinter.git",
  "author": "Vicary A. <vicary.archangel@member.mensa.org> (https://github.com/vicary)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vicary/react-native-citizen-escposprinter/issues"
  },
  "homepage": "https://github.com/vicary/react-native-citizen-escposprinter#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "plugin:jest/recommended"
    ],
    "rules": {
      "quotes": [
        "warn",
        "double"
      ]
    }
  },
  "eslintIgnore": [
    "*.js",
    "node_modules/",
    "lib/"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/eslint-plugin": "^1.3.0",
    "@types/jest": "^29.2.5",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-ft-flow": "^2.0.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.3.1",
    "metro-react-native-babel-preset": "^0.74.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-native": "^0.71.1",
    "react-native-builder-bob": "^0.20.3",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-native": "^0.69 || ^0.70 || ^0.71"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "yarn@3.3.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNCitizenEscposprinterSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  }
}
